function printedstar(noofrow)
{
    let printedstring = "";
    for(let row=1; row<= noofrow; row++)
    {
        for(let colum=1; colum<= noofrow-row; colum++)
        {
            printedstring+=" ";
        }
        for(let space=0; space< 2*row-1; space++)
        {
            printedstring+="*";
        }
        printedstring+= "\n";
    }
    return printedstring;
}
        
        
    
    

console.log(printedstar(5));
console.log(printedstar(6));