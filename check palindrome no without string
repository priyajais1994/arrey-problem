function checkpalindromeno(num)
{
    let temp = num;
     let reverse=0;
     let rem;
     while(num!=0)
     {
        rem= num%10;
        reverse= reverse*10+rem;
        num= parseInt(num/10);
     }
     console.log(reverse);
     if(temp==reverse)
     {
        console.log("palindrome no");
     }
     else{
        console.log("not palindrome no");
     }
}
checkpalindromeno(1234);
checkpalindromeno(121);


